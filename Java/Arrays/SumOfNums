package JavaProgram.Arrays.CWH;

//create an array of 5 floats and calculate their sums

public class SumOfNum {
    public static void main(String[] args) {
        float[] num = {10.4f, 45.6f, 85.9f, 44.6f, 88.7f};
        //add 'f' otherwise it will consider it as a double and an underline will appear

        float sum=0;

        for (int i = 0; i <= num.length - 1; i++) {
           sum=sum+num[i];
        }
        System.out.println("The sum of the Array is : "+sum);
    }
}

/*
 USING FOR-EACH LOOP
 
 float sum=o;
 for(float elements : num){ //elements is user defined
 sum=sum+elements;
 }
 System.out.println("The sum of the Array is : "+sum);

*/
